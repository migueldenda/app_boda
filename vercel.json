{
  "version": 2,
  "builds": [
    {
      "src": "proyectoboda/proyectoboda/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "proyectoboda/proyectoboda/wsgi.py"
    }
  ],
  "env": {
    "PYTHONPATH": "/var/task/proyectoboda"
  }
}